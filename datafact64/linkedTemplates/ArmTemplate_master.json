{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafact64"
		},
		"AzureKeyVault_ls_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyvaultap1.vault.azure.net/"
		},
		"adls_dest_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://aldsstrg1.dfs.core.windows.net/"
		},
		"http_src_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/AARYA2k04/demo_project/refs/heads/main/"
		},
		"http_src_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "azureuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafact64_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureKeyVault_ls_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_ls_properties_typeProperties_baseUrl')]"
					},
					"adls_dest_ls_properties_typeProperties_url": {
						"value": "[parameters('adls_dest_ls_properties_typeProperties_url')]"
					},
					"http_src_ls_properties_typeProperties_url": {
						"value": "[parameters('http_src_ls_properties_typeProperties_url')]"
					},
					"http_src_ls_properties_typeProperties_userName": {
						"value": "[parameters('http_src_ls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}